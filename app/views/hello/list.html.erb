<table>
  <tr>
    <th>ISBNコード</th>
    <th>書名</th>
    <th>価格</th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.isbn %></td>
      <td><%= book.title %></td>
      <td><%= book.price %>円</td>
      <td><%= book.publish %></td>
      <td><%= book.published %></td>
      <td><%= book.dl %></td>
    </tr>
  <% end %>
</table>
